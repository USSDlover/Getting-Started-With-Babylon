{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "./",
    "module": "commonjs",
    "noResolve": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "preserveConstEnums":true,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es6"
    ],
    "rootDir": "src",
    "paths": {
      "assets/*": [
        "src/assets/*"
      ]
    }
  }
}
